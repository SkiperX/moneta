                        a.filter-toggle__link(href="" title="Показать списком")
                            +svg('list')
                        a.filter-toggle__link(href="" title="Показать таблицей")
                            +svg('tabl')



                .col-12.col-sm-6.hidden-lg-up.text-right
                    .mobile-filter Фильтры
                        i.sprite.icon-controls
